{
  "extends": "@tsconfig/svelte/tsconfig.json",

  "include": [ "Pages/*", "Pages/**/*", "App/*", "App/**/*" ],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*", "wwwroot/*", "bin/*", "obj/*", "Properties/*", ".vscode/*"],

  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "ES2021.String"
    ],
    "types": [
      "node"
    ]
  }
}
