@page
@attribute [RazorCompiledItemMetadata("RouteTemplate", Urls.StatisticsUrl)]
@using Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Razor.Hosting
@using StructuredLogExplorer
@model StructuredLogExplorer.Pages.StatisticsModel
@{
    ViewData["Title"] = "Statistics";
}

@section HeadSection {
    <link href="~/build/statistics.css" asp-append-version="true" rel="stylesheet" />
    <script defer src="~/build/statistics.js" asp-append-version="true"></script>
}

<input id="title" type="hidden" value="@ViewData["Title"]" />